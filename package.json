{
    "name": "survey",
    "version": "0.1.0",
    "private": false,
    "license": "MIT",
    "engines": {
        "node": "16.x"
    },
    "dependencies": {
        "@chakra-ui/icons": "^1.1.7",
        "@chakra-ui/react": "^1.8.6",
        "@emotion/react": "^11",
        "@emotion/styled": "^11",
        "axios": "^0.26.1",
        "axois": "^0.0.1-security",
        "body-parser": "^1.20.0",
        "concurrently": "^7.1.0",
        "cors": "^2.8.5",
        "express": "^4.17.3",
        "formik": "^2.2.9",
        "framer-motion": "^6.2.8",
        "mongoose": "^6.2.9",
        "nodemon": "^2.0.15",
        "react": "^17.0.0",
        "react-dom": "^17.0.0",
        "react-router-dom": "^6.2.2",
        "react-scripts": "^5.0.0",
        "web-vitals": "^2.1.4",
        "yup": "^0.32.11"
    },
    "scripts": {
        "build": "react-scripts build",
        "startReact": "react-scripts start",
        "heroku-postbuild": "yarn install && npm run build",
        "server": "nodemon src/backend/server.js",
        "develop": "concurrently --kill-others-on-fail \"npm run server\" \"npm run startReact \"",
        "start": "concurrently --kill-others-on-fail \"npm run server\" \"npm run startReact \"",
        "test": "react-scripts test --maxWorkers=2 --ci --coverage --reporters=default --reporters=jest-junit",
        "eject": "react-scripts eject",
        "prettier": "prettier **.md **.js **.json **.yml **.html --write",
        "lint:prettier": "prettier --check **.md **.js **.json **.yml **.html"
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "@testing-library/dom": "^8.11.3",
        "@testing-library/jest-dom": "^5.16.2",
        "@testing-library/react": "^12.1.3",
        "@testing-library/user-event": "^13.5.0",
        "jest-junit": "^10.0.0",
        "prettier": "2.5.1",
        "react-test-renderer": "^17.0.1"
    },
    "jest-junit": {
        "suiteName": "jest tests",
        "outputDirectory": ".",
        "outputName": "test-results/junit.xml",
        "classNameTemplate": "{classname}-{title}",
        "titleTemplate": "{classname}-{title}",
        "ancestorSeparator": " â€º ",
        "usePathForSuiteName": "true"
    },
    "proxy": "http://localhost:5000"
}
