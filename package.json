{
    "name": "survey",
    "version": "0.1.0",
    "private": false,
    "license": "MIT",
    "engines": {
        "node": "16.x"
    },
    "dependencies": {
        "@chakra-ui/react": "^1.8.6",
        "@emotion/react": "^11",
        "@emotion/styled": "^11",
        "framer-motion": "^4",
        "react": "^17.0.0",
        "react-dom": "^17.0.0",
        "react-router-dom": "^6.2.2",
        "react-scripts": "^5.0.0",
        "web-vitals": "^2.1.4"
    },
    "scripts": {
        "start": "react-scripts start",
        "build": "react-scripts build",
        "test": "react-scripts test --maxWorkers=2 --ci --coverage --reporters=default --reporters=jest-junit",
        "eject": "react-scripts eject",
        "prettier": "prettier **.md **.js **.json **.yml **.html **.css --write",
        "lint:prettier": "prettier --check **.md **.js **.json **.yml **.html **.css"
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "@testing-library/jest-dom": "^5.16.2",
        "@testing-library/react": "^12.1.3",
        "jest-junit": "^10.0.0",
        "prettier": "2.5.1",
        "react-test-renderer": "^17.0.1"
    },
    "jest-junit": {
        "suiteName": "jest tests",
        "outputDirectory": ".",
        "outputName": "test-results/junit.xml",
        "classNameTemplate": "{classname}-{title}",
        "titleTemplate": "{classname}-{title}",
        "ancestorSeparator": " â€º ",
        "usePathForSuiteName": "true"
    }
}
